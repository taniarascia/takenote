{
  "name": "takenote-server",
  "version": "0.0.1",
  "description": "Takenote Server",
  "author": "Tania Rascia",
  "private": false,
  "main": "src/server/index.js",
  "licence": "MIT",
  "scripts": {
    "dev": "concurrently 'npm run server' 'npm run client'",
    "client": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.dev.js",
    "server": "nodemon",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "start": "cross-env NODE_ENV=production node -r ts-node/register/transpile-only src/server/index.ts",
    "test": "jest",
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taniarascia/takenote"
  },
  "keywords": [
    "notes",
    "notes-app",
    "note-taking",
    "markdown",
    "markdown-editor",
    "redux",
    "react",
    "typescript",
    "react-hooks",
    "react-hooks-redux",
    "github"
  ],
  "bugs": {
    "url": "https://github.com/taniarascia/takenote/issues"
  },
  "homepage": "https://takenote.dev",
  "prettier": {
    "bracketSpacing": true,
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.2.3",
    "axios": "^0.19.1",
    "codemirror": "^5.51.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mousetrap": "^1.6.3",
    "mousetrap-global-bind": "^1.1.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-codemirror2": "^6.0.0",
    "react-dom": "^16.12.0",
    "react-feather": "^2.0.3",
    "react-helmet-async": "^1.0.4",
    "react-markdown": "^4.3.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^5.0.2",
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/react": "^9.4.0",
    "@types/axios": "^0.14.0",
    "@types/codemirror": "0.0.82",
    "@types/compression": "^1.0.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.45",
    "@types/jest": "^24.9.0",
    "@types/lodash": "^4.14.149",
    "@types/moment": "^2.13.0",
    "@types/node": "^13.1.8",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet-async": "^1.0.3",
    "@types/react-redux": "^7.1.6",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/uuid": "^3.4.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.2",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "patch-package": "^6.2.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  }
